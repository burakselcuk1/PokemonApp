<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pokemon_nav"
    app:startDestination="@id/overlayFragment">

    <fragment
        android:id="@+id/overlayFragment"
        android:name="com.example.pokemonapp.ui.overlayFragment.OverlayFragment"
        android:label="fragment_overlay"
        tools:layout="@layout/fragment_overlay" >
        <action
            android:id="@+id/action_overlayFragment_to_mainPageFragment2"
            app:destination="@id/mainPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainPageFragment"
        android:name="com.example.pokemonapp.ui.mainPage.MainPageFragment"
        android:label="fragment_main_page"
        tools:layout="@layout/fragment_main_page" />
</navigation>